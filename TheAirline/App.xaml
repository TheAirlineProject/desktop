<Application x:Class="TheAirline.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TheAirline.GUIModel.MasterPageModel"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <local:SharedResourceDictionary Source="GUIModel\MasterPageModel\standardResources.xaml"></local:SharedResourceDictionary>
                <local:SharedResourceDictionary Source="GUIModel\MasterPageModel\standardStyles.xaml"></local:SharedResourceDictionary>
                <local:SharedResourceDictionary Source="GUIModel\MasterPageModel\standardTemplates.xaml"></local:SharedResourceDictionary>
                <local:SharedResourceDictionary Source="GUIModel\MasterPageModel\standard.xaml"></local:SharedResourceDictionary>
                </ResourceDictionary.MergedDictionaries>

        </ResourceDictionary>


    </Application.Resources>
</Application>
