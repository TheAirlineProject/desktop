<window:PopUpWindow x:Class="TheAirline.GUIModel.CustomControlsModel.PopUpWindowsModel.PopUpInviteAlliance"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:window="clr-namespace:TheAirline.GraphicsModel.UserControlModel.PopUpWindowsModel"
                   Title="PopUpInviteAlliance" Height="300" Width="300">
    <Window.Resources>
        <DataTemplate x:Key="AirlineItem">
            <WrapPanel>
                <Image Source="{Binding Profile.Logo,Mode=OneTime}" VerticalAlignment="Bottom" RenderOptions.BitmapScalingMode="HighQuality" Width="32" Height="32" Margin="2 0 5 0"></Image>
                <TextBlock Width="150" VerticalAlignment="Bottom" Text="{Binding Profile.Name,Mode=OneTime}"/>

            </WrapPanel>
        </DataTemplate>
    </Window.Resources>
    <Grid>

    </Grid>
</window:PopUpWindow>

